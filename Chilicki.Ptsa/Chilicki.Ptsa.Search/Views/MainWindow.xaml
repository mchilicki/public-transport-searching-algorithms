<Window x:Class="Chilicki.Ptsa.Search.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chilicki.Ptsa.Search.Views"
        mc:Ignorable="d"
        Title="Public Transport Search Algorithms" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>            
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button 
            Click="ImportGtfs1_Click"
            Grid.Column="0" 
            Grid.Row="0">
            Import GTFS 1
        </Button>
        <Button 
            Click="ImportGtfs2_Click"
            Grid.Column="0" 
            Grid.Row="1">
            Import GTFS 2
        </Button>
    </Grid>
</Window>
